pipeline {
    agent any
    stages {
        stage('Setup') {
            steps {
                sh "bash jenkins/setup.sh"
            }
        }
        stage('Test') {
            steps {
                sh "bash jenkins/test.sh"
            }
        }
        stage('Build') {
            steps {
                sh "bash jenkins/Build.sh"
            }
        }
        stage('Push') {
            steps {
                sh "bash jenkins/Push.sh"
            }
        }    
        stage('Deploy') {
            steps {
                sh "bash jenkins/Deploy.sh"
            }
        }
    }
}